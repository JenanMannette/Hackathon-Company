<a href="javascript:history.back()">Go Back</a>

<h1>Company Board</h1>

{{#each companies}}
    <div>

    <h2>
        <a href="http://{{this.companies.website}}">{{this.companies.name}}</a>
    </h2>
        <h4>{{this.companies.ceo.title}}</h4>
        <h4>{{this.companies.ceo.name}}</h4>
        <a href="http://www.glassdoor.com/Job/jobs.htm?suggestCount=10&suggestChosen=false&clickSource=searchBtn&typedKeyword={{this.companies.name}}&sc.keyword={{this.companies.name}}&locT=&locId=&jobType=all">jobs</a>

        <img src="{{this.companies.squareLogo}}" alt=""/>
    </div>
    <hr/>

{{/each}}
